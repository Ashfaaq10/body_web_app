{
    "version": 2,
    "builds": [
      {
        "src": "api/*.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/upload",
        "dest": "/api/upload.js"
      },
      {
        "src": "/api/upload-base64",
        "dest": "/api/upload-base64.js"
      }
    ]
  }
  